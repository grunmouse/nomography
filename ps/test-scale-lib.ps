%!PS-Adobe-3.0


%ps2pdf -dNOSAFER 

(../../ps-steal/steal.ps) run

(../../ps-eskd/) stealfolder
(../../ps-eskd-font/) stealfolder
(./)(scale-lib.ps) steal

/cross {
	gsave
		currentpoint translate
		-5 0 moveto
		5 0 lineto
		0 -5 moveto
		0 5 lineto
		stroke
	grestore
} def


in2mm



0.2 setlinewidth
/eskd-b-font  findfont	8 scalefont setfont


100 140 moveto cross
(Строка 3) 
dup 
utf8stringsize % size
2dup
0.5 0 0 0.5 makepoint %size, target
2dup /target 2edef
90 %угол
bordervector 2neg
%2pop
rmoveto cross
target 2neg
rmoveto utf8show

100 160 moveto cross
(Строка 4) 0.5 0 0 0.5 90 showlabel

100 100 moveto cross
/a 30 def
/b 30 def
gsave
	currentpoint translate
	a rotate
	5 0 lineto
	2 0 rmoveto
	gsave stroke grestore
	b neg rotate
	%cross
	mnemonic begin
	(Строка 5) right middle b showlabel
	end
grestore



100 120 moveto cross
mnemonic begin

(Строка 6) right middle 0 180 2 5 labelmark

end


showpage